<?php

/**
 * @file
 * Contains pigeonsworld.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function pigeonsworld_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the pigeonsworld module.
    case 'help.page.pigeonsworld':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('My Awesome Module') . '</p>';
      return $output;

    default:
  }
}

/**
 * Implements hook_theme().
 */
function pigeonsworld_theme() {
  return [
    'pigeonsworld' => [
      'render element' => 'children',
    ],
  ];
}
